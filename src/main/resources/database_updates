ALTER TABLE "tokenUpdates" ADD "tokenAmount" INTEGER;
ALTER TABLE "tokenUpdates" RENAME "modId" TO "creatorId"; CREATE TABLE "bank" ("guildId" INTEGER NOT NULL UNIQUE, "amount" INTEGER NOT NULL DEFAULT 0, "transferCut" INTEGER DEFAULT 10, "maxTransfer" INTEGER DEFAULT 100, "exchangeRate" REAL DEFAULT 0, "minExchange" INTEGER DEFAULT 30, "maxExchange" INTEGER DEFAULT 100, "logChannel" INTEGER); ALTER TABLE "tokenUpdates" RENAME "data" TO "reason";
CREATE TABLE "items" ("id" INTEGER, "guildId" INTEGER NOT NULL, "name" TEXT NOT NULL, "description" INTEGER, "cost" INTEGER NOT NULL, "amount" INTEGER, PRIMARY KEY("id" AUTOINCREMENT));
